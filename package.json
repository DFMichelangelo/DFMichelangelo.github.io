    "markdown-it-footnote": "^3.0.3",
    "markdown-it-github-alerts": "^0.3.0",
    "nprogress": "^0.2.0",
    "unplugin-auto-import": "^0.17.5",
    "vue": "^3.4.15",
    "vue-router": "4.2.5",
    "vue-router-better-scroller": "^0.0.0",
    "markdown-it-magic-link": "^0.1.4"
  },
