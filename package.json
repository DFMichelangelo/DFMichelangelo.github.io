{
  "type": "module",
  "private": true,
  "packageManager": "pnpm@8.15.1",
  "scripts": {
    "build": "cross-env NODE_ENV=production vite-ssg build && cp _dist_redirects dist/_redirects && cp 404.html dist/404.html",
    "dev": "vite --port 3333 --open",
    "preview": "vite preview",
    "build-and-preview": "npm run build && npm run preview",
    "static": "rimraf temp && degit dfmichelangelo/static temp --force",
    "redirects": "esno scripts/redirects.ts",
    "lint": "eslint .",
    "compress": "esno scripts/img-compress-staged.ts",
    "check-links": "find pages -name \\*.md -print0 | xargs -0 -n1 markdown-link-check --config ./markdownLinkCheckConfig.json --quiet"
  },
